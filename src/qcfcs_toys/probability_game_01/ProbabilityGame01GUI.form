<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="qcfcs_toys.probability_game_01.ProbabilityGame01GUI">
  <grid id="27dc6" binding="panel1" default-binding="true" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none" title="Probabilty Game 01" title-justification="2" title-position="1"/>
    <children>
      <grid id="4d2ae" binding="GameTypePanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="line">
          <color color="-16777216"/>
        </border>
        <children>
          <component id="d711d" class="javax.swing.JRadioButton" binding="BooleanGameButton">
            <constraints/>
            <properties>
              <text value="Boolean"/>
            </properties>
          </component>
          <component id="6fae3" class="javax.swing.JRadioButton" binding="RealGameButton">
            <constraints/>
            <properties>
              <text value="Real"/>
            </properties>
          </component>
          <component id="eb07b" class="javax.swing.JRadioButton" binding="ComplexGameButton">
            <constraints/>
            <properties>
              <text value="Complex"/>
            </properties>
          </component>
          <component id="9ec24" class="javax.swing.JLabel" binding="NumStateLabel">
            <constraints/>
            <properties>
              <text value="States = "/>
            </properties>
          </component>
          <component id="8430b" class="javax.swing.JFormattedTextField" binding="NumStatesTextField">
            <constraints/>
            <properties>
              <text value="1"/>
            </properties>
          </component>
          <component id="c4677" class="javax.swing.JButton" binding="StartButton">
            <constraints/>
            <properties>
              <text value="Start"/>
            </properties>
          </component>
