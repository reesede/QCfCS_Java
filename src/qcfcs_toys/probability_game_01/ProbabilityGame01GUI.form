      </grid>
      <grid id="70fd7" binding="ControlPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="line">
          <color color="-16777216"/>
        </border>
        <children>
          <component id="785e8" class="javax.swing.JLabel" binding="IterationCountLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="Iteration Count = 0"/>
            </properties>
          </component>
          <component id="17c4a" class="javax.swing.JButton" binding="ExecuteButton">
            <constraints border-constraint="East"/>
            <properties>
              <text value="Execute"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="b9c4d" binding="MainPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="Center"/>
        <properties>
          <preferredSize width="400" height="400"/>
          <requestFocusEnabled value="false"/>
        </properties>
        <border type="line">
          <color color="-16777216"/>
        </border>
        <children>
          <scrollpane id="ac9b" binding="MainScrollPane">
            <constraints/>
            <properties>
              <horizontalScrollBarPolicy value="32"/>
              <preferredSize width="500" height="280"/>
              <requestFocusEnabled value="false"/>
              <verticalScrollBarPolicy value="22"/>
            </properties>
            <border type="none"/>
            <children>
              <grid id="7d539" binding="GamePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
                <constraints/>
                <properties/>
                <border type="none"/>
                <children>
                  <grid id="97470" binding="MatrixPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
                    <constraints border-constraint="West"/>
                    <properties/>
                    <border type="none"/>
                    <children/>
                  </grid>
                  <grid id="ee261" binding="StatePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
                    <constraints border-constraint="East"/>
                    <properties/>
                    <border type="none"/>
                    <children/>
                  </grid>
                </children>
              </grid>
